    <style name="CarApp">
        <item name="android:windowBackground">#50000000</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>
    <style name="MyCheckView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:background">@drawable/selector_switch_bg</item>
    </style>
    <declare-styleable name="AutoWrapTextViewStyle">
        <attr format="dimension" name="paddingLeft"/>
        <attr format="dimension" name="paddingTop"/>
        <attr format="dimension" name="paddingRight"/>
        <attr format="dimension" name="paddingBottom"/>
        <attr format="dimension" name="textSize"/>
        <attr format="color" name="textColor"/>
        <attr format="integer" name="lineSpacingExtra"/>
    </declare-styleable>
    <declare-styleable name="FullscreenAttrs">
        <attr format="color" name="fullscreenBackgroundColor"/>
        <attr format="color" name="fullscreenTextColor"/>
    </declare-styleable>
    <declare-styleable name="MyCheckImageView">
        <attr format="integer" name="maxLevel"/>
        <attr format="integer" name="level"/>
        <attr format="reference" name="checkedImg"/>
        <attr format="reference" name="text"/>
    </declare-styleable>
